{
    "name" : "apnet/coding-standard",
    "type" : "library",
    "description" : "Apnet PHP coding standard",
    "license" : "MIT",
    "require" : {
        "squizlabs/php_codesniffer" : "~2.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.0"
    },
    "extra" : {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload" : {
        "psr-0" : {
            "Apnet_": "src/",
            "Apnet\\Tests\\": "src/",
            "Symfony2_": "src/"
        }
    },
    "config": {
        "bin-dir" : "bin"
    }
}
